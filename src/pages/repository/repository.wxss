/* pages/repository/repository.wxss */

 .wxParse {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 40rpx;
    right: 40rpx;  
    padding: 40rpx 0;
    overflow-y: scroll;
}
.over {
    overflow: hidden;
}
.wxParse-img {
    width: 100%!important;
}
video {
    height: 300rpx;
}
.wxParse-box {
    height: 300rpx;
    position: relative;
}
.wxParse-vpic {
    position: absolute;
    left: 0;
    right: 0;
    height: 300rpx;
}
.wxParse-vbtn {
    position: absolute;
    width: 80rpx;
    height: 80rpx;
    left: 50%;
    margin-left: -40rpx;
    top: 50%;
    margin-top: -40rpx;
}
.wxParse-dialog {
    position: fixed;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, .8);
    z-index: 9;
}
.wxParse-videos {
    width: 600rpx;
    height: 300rpx;
    position: absolute;
    top: 50%;
    margin-top: -150rpx;
    left: 50%;
    margin-left: -300rpx;
    z-index: 99;
}
